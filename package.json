{
  "name": "bs-react-navigation",
  "version": "0.1.0",
  "scripts": {
    "start": "bsb -make-world -w",
    "build": "bsb -make-world",
    "clean-build": "bsb -clean-world -make-world",
    "test": "exit 0",
    "build-docs": "sh ./docs/scripts/build-docs.sh",
    "start-docs": "live-server docs",
    "run-ios": "react-native run-ios --project-path \"./example/.native/ios\"",
    "run-android": "react-native run-android --root \"./example/.native\"",
    "start-example": "node ./node_modules/react-native/local-cli/cli.js start --root ./example",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "license": "MIT",
  "keywords": [
    "reason",
    "reasonml",
    "bucklescript",
    "react-native",
    "react-navigation"
  ],
  "author": {
    "name": "Luke Czyszczonik",
    "email": "lukasz.czyszczonik@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reasonml-community/bs-react-navigation.git"
  },
  "dependencies": {
    "react-navigation": "3.x"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "bs-react-native": "^0.10.0",
    "bs-platform": "^4.0.7",
    "react": "^16.3.0-alpha.1",
    "react-native": "^0.55.4",
    "reason-react": "^0.5.3"
  },
  "peerDependencies": {
    "bs-react-native": "^0.10.0",
    "reason-react": "^0.5.3"
  }
